<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd">
    <layout spacing="true">
        <browserFrame id="bfSandboxParameter"
                      width="100%"
                      height="100%"
                      sandbox=""/>

        <browserFrame id="bfSandboxParameters2"
                      width="100%"
                      height="100%"
                      sandbox="allow-same-origin allow-popups"/>

        <browserFrame id="bfSandboxParameters5"
                      width="100%"
                      height="100%"
                      sandbox="allow-popups allow-pointer-lock allow-top-navigation allow-scripts allow-presentation"/>

        <browserFrame id="bfSandboxCustomParameter"
                      width="100%"
                      height="100%"
                      sandbox="custom-parameter"/>

        <browserFrame id="bfSandboxCombinedParameters"
                      width="100%"
                      height="100%"
                      sandbox="allow-modals custom-parameter"/>
    </layout>
</window>