<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd">
    <layout spacing="true">
        <browserFrame id="bfReferrerPolicyParameter"
                      width="100%"
                      height="100%"
                      referrerpolicy="no-referrer"/>

        <browserFrame id="bfReferrerPolicyParameters2"
                      width="100%"
                      height="100%"
                      referrerpolicy="same-origin strict-origin"/>

        <browserFrame id="bfReferrerPolicyParameters5"
                      width="100%"
                      height="100%"
                      referrerpolicy="no-referrer-when-downgrade origin no-referrer origin-when-cross-origin strict-origin-when-cross-origin unsafe-url"/>

        <browserFrame id="bfReferrerPolicyCustomParameter"
                      width="100%"
                      height="100%"
                      referrerpolicy="custom-parameter"/>

        <browserFrame id="bfReferrerPolicyCombinedParameters"
                      width="100%"
                      height="100%"
                      referrerpolicy="unsafe-url custom-parameter"/>
    </layout>
</window>